<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - WareHouse</title>
@*     <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" /> *@
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/WareHouse.styles.css" asp-append-version="true" />
</head>
<body>
    <nav class="container">
        <div class="title-left">
            <a asp-controller="Home" asp-action="">WearHouse</a>
            <a asp-area="" asp-controller="Products" asp-action="">Products</a>
            <a asp-area="" asp-controller="AboutUs" asp-action="AboutUs">About Us</a>
            @* <a asp-area="" asp-controller="AboutUs" asp-action="">About Us</a> *@
        </div>

        <div class="title-right">
            <a>Login</a>
            <a>Register</a>
        </div>
    </nav>

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    @* <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - WareHouse - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer> *@

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
   @*  <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script> *@
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
